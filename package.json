{
  "name": "boilerplate",
  "version": "1.1.0",
  "description": "framework to start a new web project",
  "private": true,
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "start": "gulp serve",
    "start:prod": "gulp serve:prod",
    "lint": "gulp lint",
    "build": "gulp && npm run build-storybook && npm run deploy-storybook",
    "precommit": "npm run lint",
    "prepush": "npm run build",
    "test": "jest",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o dist/storybook",
    "deploy-storybook": "gulp storybook:deploy"
  },
  "devDependencies": {
    "@storybook/addon-options": "^3.2.3",
    "@storybook/react": "^3.2.3",
    "babel-core": "^6.17.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.5.2",
    "browser-sync": "^2.2.1",
    "del": "^2.2.0",
    "eslint": "^3.7.1",
    "eslint-plugin-react": "^7.1.0",
    "expose-loader": "^0.7.3",
    "fs": "0.0.1-security",
    "globals-loader": "0.0.3",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.1",
    "gulp-cache": "^0.4.2",
    "gulp-cssnano": "^2.0.0",
    "gulp-eslint": "^3.0.0",
    "gulp-iconfont": "^8.0.1",
    "gulp-iconfont-css": "^2.1.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^3.0.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^3.1.0",
    "gulp-sass-lint": "^1.3.2",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-uglify": "^2.1.0",
    "gulp-useref": "^3.0.0",
    "html-loader": "^0.4.4",
    "http-proxy-middleware": "^0.17.4",
    "husky": "^0.13.2",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "jest": "^20.0.4",
    "ngrok": "^2.2.6",
    "panini": "^1.4.0",
    "path": "^0.12.7",
    "react-test-renderer": "^15.6.1",
    "run-sequence": "^1.2.2",
    "script-loader": "^0.7.0",
    "webpack": "^2.2.1",
    "webpack-stream": "^3.2.0",
    "wiredep": "^4.0.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "dependencies": {
    "es6-promise": "^4.0.5",
    "foundation-sites": "^6.3.0-rc1",
    "hamburgers": "^0.7.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.1.1",
    "js-cookie": "^2.1.3",
    "lodash": "^4.16.4",
    "lory.js": "^2.2.1",
    "material-ui": "^0.19.4",
    "normalize-scss": "^6.0.0",
    "object-fit-images": "^3.1.3",
    "react": "^15.6.1",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-tabs": "^1.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "url": "^0.11.0"
  }
}
